{
  "name": "tableschema-ui",
  "version": "1.1.0",
  "license": "MIT",
  "author": "OKI",
  "description": "UI components for tableschema",
  "homepage": "https://github.com/frictionlessdata/tableschema-ui",
  "bugs": "https://github.com/frictionlessdata/tableschema-ui/issues",
  "repository": "github:frictionlessdata/tableschema-ui",
  "main": "lib/index.js",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "test"
  ],
  "scripts": {
    "analyze": "webpack --profile --json | webpack-bundle-size-analyzer",
    "build": "npm run compile && npm run bundle",
    "bundle": "webpack --output-pathinfo --progress --hide-modules && NODE_ENV=production webpack --progress --hide-modules",
    "compile": "babel src --out-dir lib",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "webpack-dev-server --open",
    "format": "prettier --write '{src,test}/**/*.js' && eslint --fix '{src,test}/**/*.js'",
    "lint": "prettier --check '{src,test}/**/*.js' && eslint '{src,test}/**/*.js'",
    "pretest": "npm run lint",
    "readme": "doctoc --maxlevel 3 README.md",
    "test": "jest --coverage && JEST_ENV=jsdom jest",
    "update": "ncu -u"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "immer": "^6.0.3",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-sortable-hoc": "^1.11.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "tableschema": "^1.11.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "coveralls": "^3.0.11",
    "css-loader": "^3.5.1",
    "doctoc": "^1.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-html": "^6.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "npm-check-updates": "^4.1.2",
    "prettier": "^2.0.4",
    "react-hot-loader": "^4.12.20",
    "referencer": "^0.2.5",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-bundle-size-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
