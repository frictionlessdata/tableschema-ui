{
  "name": "tableschema-ui",
  "version": "1.1.0",
  "license": "MIT",
  "author": "OKI",
  "description": "UI components for tableschema",
  "homepage": "https://github.com/frictionlessdata/tableschema-ui",
  "bugs": "https://github.com/frictionlessdata/tableschema-ui/issues",
  "repository": "github:frictionlessdata/tableschema-ui",
  "main": "lib/index.js",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "test"
  ],
  "scripts": {
    "analyze": "webpack --profile --json | webpack-bundle-size-analyzer",
    "build": "npm run compile && npm run bundle",
    "bundle": "webpack --output-pathinfo --progress --hide-modules && NODE_ENV=production webpack --progress --hide-modules",
    "compile": "babel src --out-dir lib",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "webpack-dev-server --open",
    "format": "prettier --write '{src,test}/**/*.js' && eslint --fix '{src,test}/**/*.js'",
    "lint": "prettier --check '{src,test}/**/*.js' && eslint '{src,test}/**/*.js'",
    "pretest": "npm run lint",
    "readme": "doctoc --maxlevel 3 README.md",
    "test": "jest --coverage && JEST_ENV=jsdom jest",
    "update": "ncu -u"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "immer": "^1.2.0",
    "lodash": "^4.17.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-sortable-hoc": "^0.6.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "tableschema": "^1.11.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-istanbul": "^4.1.6",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.10",
    "doctoc": "^1.4.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^1.1.11",
    "husky": "^3.0.8",
    "jest": "^25.2.4",
    "npm-check-updates": "^4.1.2",
    "prettier": "^2.0.2",
    "react-hot-loader": "^4.0.0",
    "referencer": "^0.2.5",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.1.1"
  }
}
